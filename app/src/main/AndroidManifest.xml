<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yoursy.wew" >

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/wew"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Splash"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".EmptyNote"
            android:label="@string/title_activity_empty_note"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Preview_Note"
            android:label="@string/title_activity_preview__note"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".NewNote"
            android:label="@string/title_activity_new_note"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Notes"
            android:label="@string/title_activity_notes"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Edit_Note"
            android:label="@string/title_activity_edit__note"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Share"
            android:label="@string/title_activity_share"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Suggestion"
            android:label="@string/title_activity_suggestion"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
